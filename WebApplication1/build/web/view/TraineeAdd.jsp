                        <form  action="AddTrainee" method="post" >
                            <input type ="text" hidden name="classSJ" value ="${requestScope.classSJ}"/>
                            <input type ="text" hidden name="subject" value ="${requestScope.subject}"/>
                            <table class=" table table-striped table-hover">
                                <thead>
                                <style>
                                    input[type="submit"] {
                                        font-weight: bold;
                                    }

                                    input[type="submit"].custom-button {
                                        border-radius: 9px;
                                        border: 2px solid #299be4;
                                        color: #fcfcfc;
                                        background-color: #299be4;
                                        padding: 5px 10px;
                                    }
                                </style>
                                <tr>
                                    <th>Username</th>
                                    <th>Fullname</th>
                                    <th>Email</th>
                                    <th>Action</th>

                                </tr>

                                </thead>
                                <tbody>
                                    <c:forEach var="accountnotclass" items="${list}">
                                        <tr>

                                    <input type="hidden" name="username" value="${accountnotclass.username}">
                                    <input type="hidden" name="fullname" value="${accountnotclass.fullname}">
                                    <input type="hidden" name="email" value="${accountnotclass.email}">

                                    <td>${accountnotclass.username}</td>
                                    <td>${accountnotclass.fullname}</td>
                                    <td>${accountnotclass.email}</td>
                                    <td><input type="checkbox" value="${accountnotclass.username}" name="isAdd"></td>
                                    </tr>
                                </c:forEach>
                                </tbody>
                            </table>
                            <input type="submit" value="Add" class="custom-button" />
                        </form>

                        <div class="clearfix">
                            <div class="hint-text">Showing <b>${requestScope.list.size()}</b></div>

                            <ul class="pagination">
                                <li class="page-item"><a href="TraineeList?page=${(page - 1) < 1?(1):(page-1)}&class=${requestScope.c.getClassID()}&subject=${requestScope.c.getS().getSubjectID()}">Previous</a></li>
                                    <c:forEach begin = "${1}" end = "${totalPage}" var = "i">
                                    <li class="page-item"><a class ="${i == page ? "active":"noActive"}" href ="TraineeList?page=${i}&class=${requestScope.c.getClassID()}&subject=${requestScope.c.getS().getSubjectID()}">${i}</a></li>
                                    </c:forEach>

                                <li class="page-item"><a href="TraineeList?page=${(page + 1) > totalPage?(1):(page+1)}&class=${requestScope.c.getClassID()}&subject=${requestScope.c.getS().getSubjectID()}" >Next</a></li>
